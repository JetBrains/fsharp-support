<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net461</TargetFramework>
        <RootNamespace>JetBrains.ReSharper.Plugins.FSharp.TypeProviders.Protocol</RootNamespace>
        <AssemblyName>JetBrains.ReSharper.Plugins.FSharp.TypeProviders.Protocol</AssemblyName>
        <LangVersion>$(CSharpLanguageVersion)</LangVersion>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="FSharp.Core" Version="$(FSharpCoreVersion)" />
        <PackageReference Include="JetBrains.FSharp.Compiler.Service" Version="$(FSharpCompilerServiceVersion)" />
    </ItemGroup>

    <Import Project="$(DotNetSdkPath)\Build\SubplatformReference.Rider_RdBackend.Common.Props" Condition="Exists('$(DotNetSdkPath)\Build\SubplatformReference.Rider_RdBackend.Common.Props')" />
    <Import Project="$(DotNetSdkPath)\Build\SubplatformReference.Rider_Rider.Backend.Props" Condition="Exists('$(DotNetSdkPath)\Build\SubplatformReference.Rider_Rider.Backend.Props')" />
</Project>
